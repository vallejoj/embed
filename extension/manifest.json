{
  "name": "docdoctor",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ],

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "browser_action": {
    "default_title": "DocDoctor",
    "default_popup": "popup.html"
  },

  "permissions": ["tabs"]
}
